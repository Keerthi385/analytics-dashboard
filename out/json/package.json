{
  "name": "analytics-dashboard",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "postinstall": "turbo prune --scope=api --docker && cd apps/api && npx prisma generate"
  },
  "devDependencies": {
    "turbo": "^1.6.5",
    "typescript": "^5.6.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "recharts": "^3.3.0",
    "shadcn-ui": "^0.9.5",
    "tailwindcss": "^4.1.17"
  }
}
